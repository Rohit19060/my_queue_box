{
	"name": "svelte-youtube-watch-later",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --open",
		"build": "vite build",
		"preview": "vite preview --host --open",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest run src/index.test.ts src/lib/index.test.ts src/lib/stores/stores.test.ts src/routes/components/components.test.ts src/routes/api/youtube/+server.test.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.48.2",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.8.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"gh-pages": "^6.2.0",
		"globals": "^15.12.0",
		"postcss": "^8.4.49",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.8",
		"svelte": "^5.2.2",
		"svelte-check": "^4.0.9",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.14.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.5"
	},
	"type": "module",
	"dependencies": {
		"@vercel/analytics": "^1.4.0"
	}
}